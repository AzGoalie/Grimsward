{
  "name": "Grimsward",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "fmt:check": "clojure -Sdeps '{:deps {cljfmt/cljfmt {:mvn/version \"0.7.0\"}}}' -M -m cljfmt.main check",
    "fmt:fmt": "clojure -Sdeps '{:deps {cljfmt/cljfmt {:mvn/version \"0.7.0\"}}}' -M -m cljfmt.main fix",
    "dev": "shadow-cljs watch app",
    "release": "shadow-cljs release app",
    "server": "shadow-cljs server",
    "analyize": "shadow-cljs run shadow.cljs.build-report app report.html",
    "clean": "rm -rf target && rm -rf public/js",
    "clean-win": "rmdir /s /q public/js & rmdir /s /q target"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.29",
    "@material-ui/icons": "^5.0.0-alpha.28",
    "firebase": "^8.3.2",
    "highlight.js": "10.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "shadow-cljs": "^2.12.1"
  }
}
